		footer{
			text-align: center;
			/*margin: 41em;*/position: fixed;
		    top: 78%;
		    left: 50%;
		    transform: translate(-50%, -50%);
		}
		.top-img{
			position: fixed;
			top: 20%;
			left: 50%;
			transform: translate(-50%, -50%);
